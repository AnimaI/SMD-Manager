# Flask Settings
FLASK_APP=app.py
FLASK_ENV=production
FLASK_DEBUG=False

# Database Settings
DATABASE_URI=sqlite:///smd_inventory.db

# DigiKey API Credentials
DIGIKEY_CLIENT_ID=your_client_id_here
